<UserControl xmlns:my="clr-namespace:GreenField.Common;assembly=GreenField.Common"  x:Class="GreenField.Module.Views.DashboardView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit" 
    xmlns:SLToolkit="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls"    
    xmlns:local="clr-namespace:GreenField.Common.DragGridPanelControls;assembly=GreenField.Common"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400">
    
    <Grid x:Name="LayoutRoot" Background="White">
        <my:DashboardUserControl x:Name="ucDashBoard" UserPreferenceList="{Binding UserPreferenceList, Mode=TwoWay}" 
                                 DataSource="{Binding ItemSourceList}"/>
    </Grid>
</UserControl>
